package com.imooc.test;

public class Cat extends Animal {

//	public int temp = 300;
	public int temp;
	
	static {
		System.out.println("子类的静态代码块");
	}
	
	{
		System.out.println("子类的构造代码块");
	}
	
	public Animal() {
		System.out.println("子类的构造无参构造方法");
	}
	
	public Animal(int temp) {
		System.out.println("子类的构造有参构造方法");
	}		
	
}
