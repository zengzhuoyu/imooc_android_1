package com.imooc;

public class testDemo {

	public static void main(String[] args) {

		int[] a = {34,53,12,32,56,17};
		int temp;
		for(int i = 0;i < a.length-1;i++) {
			
			for(int j = 0;j < a.length-i-1;j++){
				
				if(a[j] > a[j+1]) {
					temp = a[j];
					a[j] = a[j+1];
					a[j+1] = temp;
				}
			}
			
		}
		
		for(int n:a) {
			System.out.println(n+" ");
		}
		
	}

}
