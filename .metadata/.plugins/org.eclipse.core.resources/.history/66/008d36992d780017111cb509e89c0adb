package com.imooc.test;

public class Danli {

	//static作用:该属性在类加载的时候就生成了，只生成一个该属性的引用地址
	//实现目的：1.只能有一个实例；2.自行创建实例
	private static Danli instance = new Danli();
	
	//实现目的：只能有一个实例
	private Danli() {}
	
	//实现目的：1.只能有一个实例；2.自行创建实例
	public static Danli getInstance() {
		//静态方法中不能用this
		return instance;
	}
}
